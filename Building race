#include <bits/stdc++.h>
using namespace std;
void solve(float a,float b,float x,float y){
    float timea=a/x;
    float timeb = b/y;
    if(timea>timeb){
        cout << "Chefina"<<endl;
    }else if(timea==timeb){
        cout << "Both"<<endl;
    }else
    cout << "Chef"<<endl;
}
int main()
{
    int t;
    cin >> t;
    while(t--){
    float a,b,x,y;
    cin>>a>>b>>x>>y;
        solve(a,b,x,y);
    }
    return 0;
}
//Building race
